<configuration>
<properties>
  <property Key="microedition.global.version"
	  Value="1.0"
	  Scope="system"
	  Comment="Version of MI18N API."/>
<!-- INTERNAL PROPERTIES -->
<!-- Visible from Configuration.getProperty() -->
  <!-- System tunable parameters -->
  <property Key="microedition.global.collation.abstractionlayer"
	  Value="com.sun.j2me.global.CollationAbstractionLayerImpl"
	  Scope="internal"
	  Comment="Abstraction layer for JSR 238 (MI18N) string collation."/>
  <property Key="microedition.global.format.abstractionlayer"
	  Value="com.sun.j2me.global.FormatAbstractionLayerImpl"
	  Scope="internal"
	  Comment="Abstraction layer for JSR 238 (MI18N) formatting."/>
  <property Key="microedition.global.resource.abstractionlayer"
	  Value="com.sun.j2me.global.ResourceAbstractionLayerImpl"
	  Scope="internal"
	  Comment="Abstraction layer for JSR 238 (MI18N) resources access."/>
  <property Key="microedition.global.root"
	  Value="global/"
	  Scope="internal"
	  Comment="Device resources root for JSR 238 (MI18N)."/>
  <property Key="microedition.global.common"
	  Value="common"
	  Scope="internal"
	  Comment="Common device resources base name for JSR 238 (MI18N)."/>
  <property Key="microedition.global.collation"
	  Value="_coltable, _coltableZH, _coltableJA"
	  Scope="internal"
	  Comment="Information on collation element tables for each supported"/>
  <property Key="microedition.global.collation._coltable.locales"
	  Value="\&quot;\&quot; = 0; en, en-US, he, he-IL = 3; sk, sk-SK = 4; cs, cs-CZ = 5; es, es-ES = 6"
	  Scope="internal"
	  Comment="Information on collation element tables for each supported locale"/>
  <property Key="microedition.global.collation._coltable.maxcontr"
	  Value="3"
	  Scope="internal"
	  Comment="Information on collation element tables for each supported locale"/>
  <property Key="microedition.global.collation._coltableZH.locales"
	  Value="zh, zh-CN = 1"
	  Scope="internal"
	  Comment="Information on collation element tables  for ZH locale"/>
  <property Key="microedition.global.collation._coltableZH.maxcontr"
	  Value="3"
	  Scope="internal"
	  Comment="Information on collation element tables for ZH locale"/>
  <property Key="microedition.global.collation._coltableJA.locales"
	  Value="ja, ja-JP = 2"
	  Scope="internal"
	  Comment="Information on collation element tables for JA locale"/>
  <property Key="microedition.global.collation._coltableJA.maxcontr"
	  Value="4"
	  Scope="internal"
	  Comment="Information on collation element tables for JA locale"/>
  <property Key="microedition.global.normalization"
	  Value="_nrmtable"
	  Scope="internal"
	  Comment="Normalization tables base name for JSR 238 (MI18N)."/>
  <property Key="microedition.global.normalization.maxdecomp"
	  Value="4"
	  Scope="internal"
	  Comment="Normalization tables base name for JSR 238 (MI18N)."/>

</properties>

<constants>
<!-- Package com.sun.j2me.global -->
<constant_class Package="com.sun.j2me.global" Name="Constants"
Scope="public"
Comment="/** Resources ID definitions generated by the configurator utility */">
  <constant Type="int"
            Name="NUMBER_FORMAT_SYMBOL_RESOURCE_ID"
            Value="0x7ffffffe"
            VScope="public"
            Comment="Resource ID for the Number Format Symbol Resource"/>
  <constant Type="int"
            Name="DATETIME_FORMAT_SYMBOL_RESOURCE_ID"
            Value="0x7ffffffd"
            VScope="public"
            Comment="Resource ID for the DateTime Format Symbol Resource"/>
</constant_class>
</constants>

</configuration>